      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/dragleave_event"
    },
    {
      "number": 6,
      "name": "drop",
      "continuous": false,
      "on": "drop-zone",
      "description": "fired when the element is dropped on a valid drop-zone",
      "preventDefault": "false",
      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/drop_event"
    },
    {
      "number": 7,
      "name": "dragend",
      "continuous": false,
      "on": "dragged-element",
      "description": "fired when the user finishes dragging the element",
      "preventDefault": "false",
      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/dragend_event"
