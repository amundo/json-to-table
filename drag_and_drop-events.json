      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/drag_event"
    },
    {
      "number": 3,
      "name": "dragenter",
      "continuous": false,
      "on": "drop-zone",
      "description": "fired when the dragged element enters a valid drop-zone",
      "preventDefault": "usually true",
      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/dragenter_event"
    },
    {
      "number": 4,
      "name": "dragover",
      "continuous": false,
      "on": "drop-zone",
      "description": "fired when the dragged element is over a valid drop-zone",
      "preventDefault": "usually true",
      "mdn": "https://developer.mozilla.org/en-US/docs/Web/API/HTMLElement/dragover_event"
    },
    {
      "number": 5,
      "name": "dragleave",
      "continuous": false,
      "on": "drop-zone",
